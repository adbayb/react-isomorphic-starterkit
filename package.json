{
  "name": "react-isomorphic-starterkit",
  "version": "3.0.0",
  "description": "An isomorphic React boilerplate with Webpack (SASS/CSS loader, PostCSS loader, JSX/JS loader, File loader...), React Router, React Hot Loader and ESLint static code verification.",
  "scripts": {
    "postinstall": "npm start",
    "start": "npm run watch",
    "clean": "node ./bin/clean.js",
    "server": "node ./bin/server",
    "build:dev": "npm run clean && node ./bin/bundler/dev.js",
    "build:prod": "npm run clean && cross-env NODE_ENV=production node ./bin/bundler/prod.js",
    "build": "npm run build:prod",
    "deploy": "npm run build:prod && cross-env NODE_ENV=production npm run server",
    "watch": "npm run build:dev && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aybadb/react-isomorphic-starterkit.git"
  },
  "bugs": {
    "url": "https://github.com/aybadb/react-isomorphic-starterkit/issues"
  },
  "keywords": [
    "reactjs",
    "webpack",
    "react-router",
    "boilerplate",
    "starter",
    "isomorphic",
    "kit",
    "starterkit",
    "starter-kit"
  ],
  "author": "Ayoub ADIB <ayoub.development@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "node-sass": "^4.5.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.2.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "cross-env": "^3.1.4",
    "express": "^4.14.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6.9.4"
  }
}
