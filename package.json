{
  "name": "reactjs-isomorphic-starterkit",
  "version": "2.0.0",
  "description": "An isomorphic React boilerplate with Webpack (SASS/CSS loader, JSX/JS loader, File loader...), React Router, React Hot Loader and ESLint static code verification.",
  "scripts": {
    "postinstall": "npm run deploy",
    "start": "if-env NODE_ENV=production && npm run deploy || npm run watch",
    "clean": "node ./bin/clean.js",
    "server": "node ./bin/server",
    "build": "npm run clean && node ./bin/bundler/prod.js",
    "deploy": "npm run build && npm run server",
    "watch": "npm run clean && node ./bin/bundler/dev.js && concurrent \"node ./bin/server/hotReload.js\" \"nodemon ./bin/server --watch ./dist --watch ./bin/server -e js\" --kill-others"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayoubdev/reactjs-isomorphic-starterkit.git"
  },
  "bugs": {
    "url": "https://github.com/ayoubdev/reactjs-isomorphic-starterkit/issues"
  },
  "keywords": [
    "reactjs",
    "webpack",
    "react-router",
    "boilerplate",
    "starter",
    "isomorphic",
    "kit",
    "starterkit",
    "starter-kit"
  ],
  "author": "Ayoub ADIB <ayoub.development@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.2",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "node-sass": "^3.10.0",
    "nodemon": "^1.10.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "if-env": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.7.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.8.1",
    "serve-favicon": "^2.3.0"
  }
}
