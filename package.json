{
	"name": "react-isomorphic-starterkit",
	"version": "3.0.0",
	"description":
		"An isomorphic React boilerplate with Webpack (CSS loader, PostCSS loader, JSX/JS loader, File loader...), React Router, React Hot Loader and ESLint static code verification.",
	"scripts": {
		"postinstall": "npm start",
		"start": "npm run watch",
		"clean": "node ./bin/clean.js",
		"server": "node ./bin/server",
		"build:dev": "npm run clean && node ./bin/bundler/dev.js",
		"build:prod":
			"npm run clean && cross-env NODE_ENV=production node ./bin/bundler/prod.js",
		"build": "npm run build:prod",
		"deploy":
			"npm run build:prod && cross-env NODE_ENV=production npm run server",
		"watch": "npm run build:dev && npm run server",
		"format": "prettier --write **/*.{js,jsx,json,css}"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aybadb/react-isomorphic-starterkit.git"
	},
	"bugs": {
		"url": "https://github.com/aybadb/react-isomorphic-starterkit/issues"
	},
	"keywords": [
		"reactjs",
		"webpack",
		"react-router",
		"boilerplate",
		"starter",
		"isomorphic",
		"kit",
		"starterkit",
		"starter-kit"
	],
	"author": "Ayoub ADIB <adbayb@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"assets-webpack-plugin": "^3.5.1",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.0.1",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-latest": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^0.28.7",
		"del": "^3.0.0",
		"eslint": "^4.7.2",
		"eslint-plugin-prettier": "^2.3.1",
		"eslint-plugin-react": "^7.4.0",
		"extract-text-webpack-plugin": "^3.0.0",
		"file-loader": "^0.11.2",
		"postcss-cssnext": "^3.0.2",
		"postcss-loader": "^2.0.6",
		"prettier": "^1.7.2",
		"react-hot-loader": "^3.0.0-beta.6",
		"style-loader": "^0.18.2",
		"webpack": "^3.6.0",
		"webpack-dev-server": "^2.9.0",
		"webpack-merge": "^4.1.0"
	},
	"dependencies": {
		"babel-runtime": "^6.26.0",
		"compression": "^1.7.1",
		"cross-env": "^5.0.5",
		"express": "^4.15.5",
		"react": "^15.4.2",
		"react-dom": "^15.4.2",
		"react-router": "^3.0.2",
		"webpack-dev-middleware": "^1.12.0",
		"webpack-hot-middleware": "^2.19.1"
	},
	"engineStrict": true,
	"engines": {
		"node": ">= 6.4.0"
	}
}
